{
  "name": "small-preject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev",
    "test": "vue-cli-service serve --mode test",
    "build": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "deploy:prod": "npm run build && cross-env NODE_ENV=prod node ./deploy",
